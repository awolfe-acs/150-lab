{
  "name": "countdown-timer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode standard --outDir dist-aem",
    "build:gh": "vite build --mode github",
    "build:assets": "vite build --mode assets --outDir dist-assets",
    "build:banner": "vite build --mode banner --outDir dist-banner",
    "build:aem": "pwsh -Command \"if (Test-Path dist-aem) { Remove-Item -Recurse -Force dist-aem -ErrorAction SilentlyContinue; Start-Sleep -Milliseconds 500 }\" && npm run build && pwsh -File copy-to-clientlib.ps1",
    "preview": "vite preview"
  },
  "devDependencies": {
    "sass": "^1.85.1",
    "sass-embedded": "^1.85.1",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "dat.gui": "^0.7.9",
    "globe.gl": "^2.41.6",
    "gsap": "file:gsap-bonus.tgz",
    "lenis": "^1.1.20",
    "lil-gui": "^0.21.0",
    "split-type": "^0.3.4",
    "three": "^0.181.0"
  }
}
